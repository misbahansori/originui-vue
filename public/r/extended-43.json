{
  "$schema": "https://shadcn-vue.com/schema/registry-item.json",
  "name": "extended-43",
  "type": "registry:component",
  "files": [
    {
      "path": "components/extended-43.vue",
      "content": "<script setup lang=\"ts\">\nimport {\n  ContributionGraph,\n  ContributionGraphBlock,\n  ContributionGraphGroup,\n} from \"@/registry/default/ui/contribution-graph\";\nimport {\n  Tooltip,\n  TooltipContent,\n  TooltipProvider,\n  TooltipTrigger,\n} from \"@/registry/default/ui/tooltip\";\n\nconst generateData = () => {\n  const days = [];\n  const startDate = new Date();\n  startDate.setMonth(startDate.getMonth() - 12);\n\n  for (let i = 0; i < 364; i++) {\n    const currentDate = new Date(startDate);\n    currentDate.setDate(startDate.getDate() + i);\n\n    const count = Math.floor(Math.random() * 13);\n\n    days.push({\n      date: currentDate.toISOString().split(\"T\")[0],\n      count: count,\n    });\n  }\n  return days;\n};\n\nconst data = ref(generateData());\n\nconst weeks = computed(() => {\n  const result = [];\n  for (let i = 0; i < data.value.length; i += 7) {\n    result.push(data.value.slice(i, i + 7));\n  }\n  return result;\n});\n\nconst getLevel = (count: number) => {\n  if (count === 0) return 0;\n  if (count <= 2) return 1;\n  if (count <= 4) return 2;\n  if (count <= 6) return 3;\n  if (count <= 8) return 4;\n  if (count <= 10) return 5;\n  return 6;\n};\n\nconst formatDate = (dateString?: string) => {\n  if (!dateString) return \"\";\n\n  return new Date(dateString).toLocaleDateString(\"en-US\", {\n    weekday: \"short\",\n    year: \"numeric\",\n    month: \"short\",\n    day: \"numeric\",\n  });\n};\n</script>\n\n<template>\n  <div class=\"flex flex-col items-center justify-center\">\n    <div>\n      <TooltipProvider>\n        <ContributionGraph class=\"flex gap-1\">\n          <ContributionGraphGroup\n            v-for=\"(week, weekIndex) in weeks\"\n            :key=\"weekIndex\"\n          >\n            <Tooltip\n              v-for=\"(day, dayIndex) in week\"\n              :key=\"dayIndex\"\n              :delay-duration=\"0\"\n            >\n              <TooltipTrigger as-child>\n                <ContributionGraphBlock\n                  :level=\"getLevel(day.count)\"\n                  :title=\"`${day.count} contributions on ${day.date}`\"\n                />\n              </TooltipTrigger>\n              <TooltipContent\n                side=\"top\"\n                class=\"bg-foreground text-background border-foreground\"\n              >\n                <div class=\"text-xs\">\n                  <p class=\"font-medium\">\n                    {{ day.count }}\n                    {{ day.count === 1 ? \"contribution\" : \"contributions\" }}\n                  </p>\n                  <p class=\"opacity-70\">\n                    {{ formatDate(day.date) }}\n                  </p>\n                </div>\n              </TooltipContent>\n            </Tooltip>\n          </ContributionGraphGroup>\n        </ContributionGraph>\n      </TooltipProvider>\n    </div>\n    <p\n      class=\"text-muted-foreground mt-2 text-xs\"\n      role=\"region\"\n      aria-live=\"polite\"\n    >\n      With Tooltip\n    </p>\n  </div>\n</template>\n",
      "type": "registry:component"
    }
  ],
  "meta": {
