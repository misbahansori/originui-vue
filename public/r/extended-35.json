{
  "$schema": "https://shadcn-vue.com/schema/registry-item.json",
  "name": "extended-35",
  "type": "registry:component",
  "files": [
    {
      "path": "components/extended-35.vue",
      "content": "<script setup lang=\"ts\">\nimport {\n  Snippet,\n  SnippetCopyButton,\n  SnippetHeader,\n  SnippetTabsContent,\n  SnippetTabsList,\n  SnippetTabsTrigger,\n} from \"@/registry/default/ui/snippet\";\nimport { computed, ref } from \"vue\";\n\nconst commands = [\n  {\n    label: \"npm\",\n    code: \"npx next-forge@latest init\",\n  },\n  {\n    label: \"yarn\",\n    code: \"yarn dlx next-forge@latest init\",\n  },\n  {\n    label: \"pnpm\",\n    code: \"pnpx next-forge@latest init\",\n  },\n  {\n    label: \"bun\",\n    code: \"bunx next-forge@latest init\",\n  },\n] as const;\n\nconst activeTab = ref(commands[0]?.label);\n\nconst activeCommand = computed(() =>\n  commands.find((command) => command.label === activeTab.value),\n);\n</script>\n\n<template>\n  <div class=\"*:not-first:mt-2\">\n    <Snippet v-model=\"activeTab\">\n      <SnippetHeader>\n        <SnippetTabsList>\n          <SnippetTabsTrigger\n            v-for=\"command in commands\"\n            :key=\"command.label\"\n            :value=\"command.label\"\n          >\n            {{ command.label }}\n          </SnippetTabsTrigger>\n        </SnippetTabsList>\n        <SnippetCopyButton :value=\"activeCommand?.code || ''\" />\n      </SnippetHeader>\n      <SnippetTabsContent\n        v-for=\"command in commands\"\n        :key=\"command.label\"\n        :value=\"command.label\"\n      >\n        <pre class=\"truncate p-4\">{{ command.code }}</pre>\n      </SnippetTabsContent>\n    </Snippet>\n    <p\n      class=\"text-muted-foreground mt-2 text-xs\"\n      role=\"region\"\n      aria-live=\"polite\"\n    >\n      Based on Kibo Snippet component\n      <a\n        class=\"hover:text-foreground underline\"\n        href=\"https://www.kibo-ui.com/components/snippet\"\n        target=\"_blank\"\n        rel=\"noopener nofollow\"\n      >\n        Kibo UI\n      </a>\n    </p>\n  </div>\n</template>\n",
      "type": "registry:component"
    }
  ],
  "meta": {
    "tags": [
      "snippet",
      "extended",
