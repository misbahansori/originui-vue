nuItem>\n          <LucidePin :size=\"16\" class=\"opacity-60\" aria-hidden=\"true\" />\n          <span>Option 4</span>\n        </DropdownMenuItem>\n        <DropdownMenuItem>\n          <LucideUserPen :size=\"16\" class=\"opacity-60\" aria-hidden=\"true\" />\n          <span>Option 5</span>\n        </DropdownMenuItem>\n      </DropdownMenuGroup>\n      <DropdownMenuSeparator />\n      <DropdownMenuItem>\n        <LucideLogOut :size=\"16\" class=\"opacity-60\" aria-hidden=\"true\" />\n        <span>Logout</span>\n      </DropdownMenuItem>\n    </DropdownMenuContent>\n  </DropdownMenu>\n</template>\n",
